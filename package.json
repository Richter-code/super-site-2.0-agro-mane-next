{
  "name": "agro-mane-next",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "kill:3001": "fuser -k 3001/tcp 2>/dev/null || true",
    "dev": "next dev --hostname 0.0.0.0 -p 3001",
    "dev:host": "next dev -H 0.0.0.0 -p 3001",
    "dev:all": "bash scripts/dev-all.sh",
    "dev:background": "nohup pnpm dev -p 3001 > .dev.log 2>&1 & disown",
    "dev:serve": "bash scripts/dev-serve.sh",
    "tunnel:local": "bash scripts/tunnel-local.sh",
    "tunnel:ngrok": "npx --yes ngrok http 3001",
    "tunnel": "bash scripts/tunnel.sh",
    "tunnel:lt": "npx localtunnel --port 3001 --subdomain agromane",
    "build": "next build",
    "start": "next start --hostname 0.0.0.0 -p 3001",
    "start:host": "next start -H 0.0.0.0 -p 3001",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "format": "prettier -w .",
    "mobile:dev": "bash scripts/mobile-dev.sh",
    "mobile:prod": "bash scripts/mobile-prod.sh",
    "mobile:check": "bash scripts/mobile-check.sh",
    "mobile:full": "bash scripts/mobile-full.sh",
    "bmad:analisar": "bash scripts/bmad-analisar.sh",
    "bmad:planejar": "bash scripts/bmad-planejar.sh",
    "bmad:impl": "bash scripts/bmad-impl.sh",
    "bmad:qa": "bash scripts/bmad-qa.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.24",
    "lucide-react": "^0.424.0",
    "next": "14.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.66.1",
    "stripe": "^20.0.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@cspell/dict-pt-br": "^2.4.2",
    "@types/node": "^20.14.11",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "autoprefixer": "^10.4.20",
    "cspell": "^9.3.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "husky": "^9.1.1",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "shadcn-ui": "^0.8.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.7",
    "typescript": "5.5.4"
  }
